<h1>Listing all users in da system</h1>

<table>
  <tr>
    <th>U id</th>
    <th>U name</th>
    <th>U password</th>
    <th>U email</th>
    <th colspan="2"></th>
  </tr>

  <% @all_users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.password %></td>
        <td><%= user.email %></td>
      </tr>
  <% end %>
</table>

